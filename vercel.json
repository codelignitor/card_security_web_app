{
  "rewrites": [
    {
      "source": "/_next/:match*",
      "destination": "https://card-detection-businesses.vercel.app/_next/:match*"
    },
    {
      "source": "/favicon.ico",
      "destination": "https://card-detection-businesses.vercel.app/favicon.ico"
    },
    {
      "source": "/:businessname/:scanid",
      "destination": "https://card-detection-businesses.vercel.app/:businessname/:scanid"
    },
    {
      "source": "/:businessname",
      "destination": "https://card-detection-businesses.vercel.app/:businessname"
    },
    {
      "source": "/securityscan",
      "destination": "https://card-detector-react-app.vercel.app/securityscan"
    },
    {
      "source": "/securityscan/:match*",
      "destination": "https://card-detector-react-app.vercel.app/securityscan/:match*"
    }
  ]
}
